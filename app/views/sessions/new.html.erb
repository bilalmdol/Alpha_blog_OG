<h1> Login to Alpha Blog </h1>


<%= form_with scope: :session, url: login_path, local: true, data: { turbo: false } do |f| %>
  
    <p>
        <%= f.label :Email %></br>
        <%= f.text_field :email %></br>
    </p>

    <p>
        <%= f.label :Password %></br>
        <%= f.text_field :password %></br>
    </p>

    <p>
        <%= f.submit "login" %>
    </p>
<% end %>


<p>
    <%= link_to "Back", users_path %>
</p>